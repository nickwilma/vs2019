<?xml version="1.0" encoding="utf-8"?>
<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>控制項</Title>
    <Author>Microsoft Corporation</Author>
    <Description>ASP.NET AJAX 用戶端控制的程式碼片段</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>namespace</ID>
        <ToolTip>命名空間</ToolTip>
        <Default>mynamespace</Default>
      </Literal>
      <Literal>
        <ID>controlname</ID>
        <ToolTip>控制項名稱</ToolTip>
        <Default>mycontrol</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript"><![CDATA[Type.registerNamespace("$namespace$");

$namespace$.$controlname$ = function(element) {
    $namespace$.$controlname$.initializeBase(this, [element]);
}

$namespace$.$controlname$.prototype = {
    initialize: function() {
        $namespace$.$controlname$.callBaseMethod(this, 'initialize');
    },
    dispose: function() {
        $namespace$.$controlname$.callBaseMethod(this, 'dispose');
    }
}

$namespace$.$controlname$.registerClass('$namespace$.$controlname$', Sys.UI.Control);$end$
]]></Code>
  </Snippet>
</CodeSnippet>
