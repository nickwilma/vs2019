<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>per</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>for</Shortcut>
    <Description>Frammento di codice per un ciclo for</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
      <SnippetType>SurroundsWith</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>index</ID>
        <ToolTip>Indice</ToolTip>
        <Default>i</Default>
      </Literal>
      <Literal>
        <ID>max</ID>
        <ToolTip>Lunghezza massima</ToolTip>
        <Default>length</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript"><![CDATA[for (var $index$ = 0; $index$ < $max$; $index$++) {
    $selected$$end$
}]]></Code>
  </Snippet>
</CodeSnippet>
