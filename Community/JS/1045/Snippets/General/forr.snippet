<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>forr</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>forr</Shortcut>
    <Description>Fragment kodu dla odwróconej pętli for</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
      <SnippetType>SurroundsWith</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>index</ID>
        <ToolTip>Indeks</ToolTip>
        <Default>i</Default>
      </Literal>
      <Literal>
        <ID>max</ID>
        <ToolTip>Maksymalna długość</ToolTip>
        <Default>length</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript"><![CDATA[for (var $index$ = $max$ - 1; $index$ >= 0; $index$--) {
    $selected$$end$
}]]></Code>
  </Snippet>
</CodeSnippet>
