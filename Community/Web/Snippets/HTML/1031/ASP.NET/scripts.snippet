<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>scripts</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>scripts</Shortcut>
    <Description>Markupausschnitt für eine Skriptsammlung für ein scriptmanager-Steuerelement</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <FileExtensions>
      aspx;master;ascx
    </FileExtensions>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>path</ID>
        <ToolTip>path</ToolTip>
        <Default>#</Default>
      </Literal>
    </Declarations>
    <Code Language="html"><![CDATA[<scripts>
    <asp:scriptreference path="$path$" />$end$
</scripts>]]></Code>
  </Snippet>
</CodeSnippet>
