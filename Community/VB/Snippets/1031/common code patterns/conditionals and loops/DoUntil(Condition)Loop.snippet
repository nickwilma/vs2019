<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Do Until...Loop-Anweisung</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Schleife, bis eine Bedingung "True" ist.</Description>
      <Shortcut>DoUntil</Shortcut>
    </Header>
    <Snippet>
      <Imports>
      </Imports>
      <Declarations>
        <Literal>
          <ID>Condition</ID>
          <Type>Object</Type>
          <ToolTip>Ersetzen Sie dies durch einen Ausdruck, der zu "True" oder "False" ausgewertet wird.</ToolTip>
          <Default>False</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Do Until $Condition$

Loop]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
