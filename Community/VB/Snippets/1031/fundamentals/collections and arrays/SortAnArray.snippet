<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Array sortieren</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Deklariert ein Array von Zeichenfolgenobjekten, füllt es und sortiert es anschließend alphabetisch.</Description>
      <Keywords>
      </Keywords>
      <Shortcut>arrSort</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>InitialValues</ID>
          <Type>Object</Type>
          <ToolTip>Ersetzen Sie dies durch die ursprünglichen Werte des Arrays.</ToolTip>
          <Default>{"lion", "turtle", "ostrich"}</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Dim animals() As String = $InitialValues$
Array.Sort(animals)]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
