<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Teile einer Zeichenfolge entfernen </Title>
      <Author>Microsoft Corporation</Author>
      <Description>Entfernt alle Vorkommnisse einer Zeichenfolge aus einer anderen Zeichenfolge.</Description>
      <Keywords>
        <Keyword>Zeichenfolge</Keyword>
        <Keyword>Ersetzen</Keyword>
      </Keywords>
      <Shortcut>typeRemove</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>Microsoft.VisualBasic</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>ToRemove</ID>
          <Type>System.String</Type>
          <ToolTip>Ersetzen Sie dies durch die Zeichenfolge, die Sie entfernen möchten.</ToolTip>
          <Default>"and "</Default>
        </Literal>
        <Literal>
          <ID>StringWithParts</ID>
          <Type>System.String</Type>
          <ToolTip>Ersetzen Sie dies durch die Zeichenfolge, aus der Sie Teile entfernen möchten.</ToolTip>
          <Default>"Books and Chapters and Pages"</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Dim withParts As String = $StringWithParts$
Dim withoutParts As String = withParts.Replace($ToRemove$, "")]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
