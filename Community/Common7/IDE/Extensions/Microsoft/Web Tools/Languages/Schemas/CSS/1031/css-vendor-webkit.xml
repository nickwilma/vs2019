        <entry _locID="-webkit-box-reflect_below" _locAttrData="description" value="below" description="Die Reflexion wird unterhalb des Rahmenbox angezeigt."/>
        <entry _locID="-webkit-box-reflect_left" _locAttrData="description" value="left" description="Die Reflexion wird links neben der Rahmenbox angezeigt."/>
        <entry _locID="-webkit-box-reflect_right" _locAttrData="description" value="right" description="Die Reflexion wird rechts neben der Rahmenbox angezeigt."/>
