<CssModule name="SVG Painting" standard-reference="http://www.w3.org/TR/SVG11/painting.html" status="REC" pub-date="2011-08-16">

  <CssProperty _locID="text-rendering" _locAttrData="description" version="3.0" browsers="C,FF3,O9,S5" restriction="enum" description="Der Ersteller des SVG-Inhalts kann einen Implementierungshinweis darüber einfügen, welche Kompromisse bei der Textdarstellung einzugehen sind. Die‘text-rendering’-Eigenschaft stellt diese Hinweise bereit." standard-reference="http://www.w3.org/TR/SVG11/painting.html#TextRenderingProperty">

    <entry value="auto" version="3.0"/>
    <entry value="optimizeSpeed" version="3.0"/>
    <entry value="optimizeLegibility" version="3.0"/>
    <entry value="geometricPrecision" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="image-rendering" _locAttrData="description" version="3.0" browsers="C,FF3.6,O11.6,S" restriction="enum" description="Der Ersteller des SVG-Inhalts kann einen Implementierungshinweis einfügen, d.h., wie bei der Bildverarbeitung ein Kompromiss zwischen Geschwindigkeit und Qualität erzielt wird. Die 'image-rendering'-Eigenschaft liefert dem SVG-Benutzer-Agent einen Hinweis zur Optimierung des Bildrenderings." standard-reference="http://www.w3.org/TR/SVG11/painting.html#ImageRenderingProperty">

    <entry value="auto" version="3.0"/>
    <entry value="optimizeSpeed" version="3.0"/>
    <entry value="optimizeQuality" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="color-rendering" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="Der Ersteller des SVG-Inhalts kann einen Implementierungshinweis einfügen, d.h., wie bei der Farbinterpolation und beim Compositing ein Kompromiss zwischen Geschwindigkeit und Qualität erzielt wird. Die 'image-rendering'-Eigenschaft liefert dem SVG-Benutzer-Agent einen Hinweis zur Optimierung der Farbinterpolation und Compositingvorgänge." standard-reference="http://www.w3.org/TR/SVG11/painting.html#ColorRenderingProperty">

    <entry value="auto" version="3.0"/>
    <entry value="optimizeSpeed" version="3.0"/>
    <entry value="optimizeQuality" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="shape-rendering" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="Der Ersteller des SVG-Inhalts kann einen Implementierungshinweis darüber einfügen, welche Kompromisse bei der Darstellung von Vektorgrafikelementen wie 'path'-Elementen und Grundformen wie Kreisen und Rechtecken einzugehen sind." standard-reference="http://www.w3.org/TR/SVG11/painting.html#ShapeRenderingProperty">

    <entry value="auto" version="3.0"/>
    <entry value="optimizeSpeed" version="3.0"/>
    <entry value="optimizeQuality" version="3.0"/>
    <entry value="crispEdges" version="3.0"/>
    <entry value="geometricPrecision" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="fill" _locAttrData="description" version="3.0" type="color" restriction="color, enum, url" description="Die 'fill'-Eigenschaft zeichnet das Innere eines bestimmten Grafikelements." standard-reference="http://www.w3.org/TR/SVG11/painting.html#FillProperty">

    <entry value="none"/>
    <entry value="url()"/>
  </CssProperty>

  <CssProperty _locID="fill-rule" _locAttrData="description" version="3.0" restriction="enum" description="Die 'fill-rule'-Eigenschaft gibt den Algorithmus an, mit dem bestimmt wird, welche Teile der Zeichenfläche in die Form eingeschlossen werden." standard-reference="http://www.w3.org/TR/SVG11/painting.html#FillRuleProperty">

    <entry value="nonzero" version="3.0"/>
    <entry value="evenodd" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="fill-opacity" _locAttrData="description" version="3.0" restriction="number(0-1)" description="'fill-opacity' gibt die Deckkraft des Zeichenvorgangs an, durch den das Innere des aktuellen Objekts gezeichnet wird." standard-reference="http://www.w3.org/TR/SVG11/painting.html#FillOpacityProperty"/>

  <CssProperty _locID="stroke" _locAttrData="description" version="3.0" type="color" restriction="color" description="Die 'stroke'-Eigenschaft zeichnet entlang der Außenlinie des jeweiligen Grafikelements." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeProperty"/>

  <CssProperty _locID="stroke-width" _locAttrData="description" version="3.0" restriction="percentage, length, integer" description="Diese Eigenschaft gibt die Strichbreite des aktuellen Objekts an." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeWidthProperty"/>

  <CssProperty _locID="stroke-linecap" _locAttrData="description" version="3.0" restriction="enum" description="Bestimmt die Form von offenen Pfadsegmentenden, wenn diese gezeichnet werden." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeLinecapProperty">

    <entry value="butt" version="3.0"/>
    <entry value="round" version="3.0"/>
    <entry value="square" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="stroke-linejoin" _locAttrData="description" version="3.0" restriction="enum" description="Bestimmt die Form von Ecken, an denen Pfadsegmente oder Grundformen beim Zeichnen zusammentreffen." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeLinejoinProperty">

    <entry value="miter" version="3.0"/>
    <entry value="round" version="3.0"/>
    <entry value="bevel" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="stroke-miterlimit" _locAttrData="description" version="3.0" restriction="number" syntax="path { $(name): 4; }" description="Wenn zwei Liniensegmente in einem spitzen Winkel aufeinander treffen und Gehrungsverbindungen für 'stroke-linejoin' angegeben wurden, kann es vorkommen, dass die Gehrung die Stärke der Linie überragt." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeMiterLimitProperty"/>

  <CssProperty _locID="stroke-dasharray" _locAttrData="description" version="3.0" restriction="length, percentage, enum, number" description="Steuert das Muster der Striche und Lücken beim Zeichnen von Konturen." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeDashArrayProperty">

    <entry value="none" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="stroke-dashoffset" _locAttrData="description" version="3.0" restriction="percentage, length, number" description="Gibt den Offset für den Beginn der Strichelung in einem Strichmuster an." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeDashOffsetProperty"/>

  <CssProperty _locID="stroke-opacity" _locAttrData="description" version="3.0" restriction="number(0-1)" description="Legt die Deckkraft beim Zeichnen einer Randlinie für das aktuelle Objekt fest." standard-reference="http://www.w3.org/TR/SVG11/painting.html#StrokeOpacityProperty"/>

    <CssProperty _locID="marker-side" _locAttrData="description" version="3.0" restriction="enum" description="Gibt an, ob Listenelementmarkierungen sich relativ zum Listenelement oder zur Direktionalität des Listencontainers positionieren." standard-reference="http://www.w3.org/TR/css3-lists/#propdef-marker-side">

        <entry value="list-item"/>
        <entry value="list-container"/>
    </CssProperty>

  <CssProperty _locID="marker-start" _locAttrData="description" version="3.0" restriction="url" description="Legt die Pfeilspitze oder den Polymarker fest, der am ersten Scheitelpunkt eines bestimmten 'path'-Elements oder einer Grundform gezeichnet werden soll." standard-reference="http://www.w3.org/TR/SVG11/painting.html#MarkerStartProperty">

    <entry value="url()" version="3.0"/>
    <entry value="none" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="marker-end" _locAttrData="description" version="3.0" restriction="url" description="Legt die Pfeilspitze oder den Polymarker fest, der am letzten Scheitelpunkt gezeichnet werden soll." standard-reference="http://www.w3.org/TR/SVG11/painting.html#MarkerEndProperty">

    <entry value="url()" version="3.0"/>
    <entry value="none" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="marker-mid" _locAttrData="description" version="3.0" restriction="url" description="Legt die Pfeilspitze oder den Polymarker fest, der an jedem weiteren Scheitelpunkt gezeichnet werden soll." standard-reference="http://www.w3.org/TR/SVG11/painting.html#MarkerMidProperty">

    <entry value="url()" version="3.0"/>
    <entry value="none" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="marker" _locAttrData="description" version="3.0" restriction="url" description="Legt das Markersymbol fest, das für alle Punkte verwendet werden soll und bestimmt den Wert aller Scheitelpunkte im jeweiligen 'path'-Element bzw. der jeweiligen Grundform." standard-reference="http://www.w3.org/TR/SVG11/painting.html#MarkerProperty">

    <entry value="url()" version="3.0"/>
    <entry value="none" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="color-interpolation" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="Gibt den Farbraum für die Interpolation von Farbverläufen, Farbanimationen und Alpha-Compositing an." standard-reference="http://www.w3.org/TR/SVG11/painting.html#ColorInterpolationProperty">

    <entry value="auto" version="3.0"/>
    <entry value="sRGB" version="3.0"/>
    <entry value="linearRGB" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="color-interpolation-filters" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="Gibt den Farbraum für Bilderzeugungsvorgänge an, die über Filtereffekte ausgeführt werden." standard-reference="http://www.w3.org/TR/SVG11/painting.html#ColorInterpolationFiltersProperty">

    <entry value="auto" version="3.0"/>
    <entry value="sRGB" version="3.0"/>
    <entry value="linearRGB" version="3.0"/>
  </CssProperty>

  <CssProperty _locID="text-anchor" _locAttrData="description" version="3.0" restriction="enum" description="Wird zum Ausrichten einer Textzeichenfolge relativ zu einem angegebenen Punkt verwendet (links, zentriert oder rechts ausgerichtet)." standard-reference="http://www.w3.org/TR/SVG11/text.html#TextAnchorProperty">

    <entry value="start"/>
    <entry value="middle"/>
    <entry value="end"/>
  </CssProperty>

  <CssProperty _locID="paint-order" _locAttrData="description" version="3.0" restriction="enum" description="Steuert die Reihenfolge, in der die drei Zeichenvorgänge für Formen und Text gerendert werden: ihre Füllung, ihr Strich und alle ggf. vorhandenen Markierungen." standard-reference="https://svgwg.org/svg2-draft/painting.html#PaintOrder">

    <entry value="normal"/>
    <entry value="fill"/>
    <entry value="stroke"/>
    <entry value="markers"/>
  </CssProperty>

</CssModule>
