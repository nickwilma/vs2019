  <CssProperty _locID="justify-content"
               _locAttrData="description"
               version="3.0"
               browsers="C29,FF28,IE11,O17,S9"
               restriction="enum"
               type="justify-content"
               description="Aligns the contents of the box as a whole (as the alignment subject) within the box itself (as the alignment container) along the inline/row/main axis of the box."
               syntax="div { $(name): flex-start; }"
               standard-reference="http://www.w3.org/TR/css-align-3/#propdef-justify-content" />

